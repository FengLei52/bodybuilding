
.Navigation{
	padding: 76rpx 0 46rpx 0;
}
.Navigation image{
	width: 84rpx;
	height: 84rpx;
}
.Navigation text{
	display: block;
	font-size: 24rpx;
	color: #8F8F8F;
}
.Navigation .hot-icon{
	position: absolute;
	top: -36rpx;
	right: 20rpx;
	width: 60rpx;
	height: 36rpx;
	z-index: 6;
}
.healthy{
	box-sizing: border-box;
	padding: 96rpx 22rpx 56rpx;
}
.health-back{
	height: 192rpx;
	background: url('http://m.qpic.cn/psc?/V14MCMD02ANAGG/OZ*kzmW4N1STRbYPSVQfBImD7jRpBR.BxzEV0y3O450nO*zz5ab.R.SVbPdcUQa4TkKmkySeVEG5wO5EIW0tYZKhRIoy998wbE.WQ3Pdejo!/b&bo=IAQgAQAAAAADFzc!&rf=viewer_4') no-repeat;
	background-size: cover;
	padding: 0 24rpx;
}
.health-text{
	font-size: 32rpx;
}
.join-text{
	font-size: 26rpx;
	color: #F9CCA8;
}
.join-icon{
	display: inline-block;
	width: 28rpx;
	height: 26rpx;
	position: relative;
	left: 6rpx;
	top: 2rpx;
}
.health-house{
	position: absolute;
	right: 24rpx;
	top: -64rpx;
	width: 200rpx;
	height: 250rpx;
}
.fire{
	display: inline-block;
	width: 28rpx;
	height: 34rpx;
	position: relative;
	top: 30rpx;
	left: 6rpx;
}
.activity-holder{
	width: 100%;
	height: 340rpx;
	white-space: nowrap;
}
.activity-item{
	display: inline-block;
	width: 296rpx;
	margin-left: 24rpx;
}
.activity-item image{
	width: 100%;
	height: 198rpx;
}
.activity-item:last-child{
	margin-right: 24rpx;
}
.activity-item .activityTitle{
	font-size: 26rpx;
	color: #5a5a5a;
	display: block;
}
.activity-item .activity-content{
	font-size: 22rpx;
	color: #474A56;
	position: relative;
	top: -6rpx;
}
.storeContainer{
	padding: 0 24rpx;
	box-sizing: border-box;
}
.sotre-item{
	margin-top: 66rpx;
	height: 192rpx;
	background: #f4f4f4;
	border-radius: 8rpx;
}
.storeImg{
	width: 192rpx;
	height: 192rpx;
	float: left;
	margin-top: -42rpx;
}
.storeInfo{
	box-sizing: border-box;
	padding-left: 22rpx;
	height: 192rpx;
	width: calc(100% - 192rpx);
}
.storeName{
	color: #333;
	font-size: 30rpx;
}
.storeSite{
	color: #F96D1B;
	font-size: 24rpx;
	float: right;
	margin-top: 8rpx;
	font-weight: bold;
}
.storeAddress{
	color: #9b9b9b;
	font-size: 24rpx;
}

